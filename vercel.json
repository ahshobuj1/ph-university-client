{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {"distDir": "dist"}
    }
  ],
  "routes": [
    {
      "src": "/(.*)\\.(js|css|png|jpg|svg|ico|json|map|woff2?)$",
      "headers": {"cache-control": "public, max-age=31536000, immutable"}
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
